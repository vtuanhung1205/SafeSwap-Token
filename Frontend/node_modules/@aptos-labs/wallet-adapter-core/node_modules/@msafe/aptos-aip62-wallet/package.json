{
  "name": "@msafe/aptos-aip62-wallet",
  "description": "MSafe Aptos Wallet",
  "version": "1.0.18",
  "license": "Apache-2.0",
  "homepage": "https://github.com/Momentum-Safe/msafe-aptos-wallet",
  "main": "dist/common/index.js",
  "module": "dist/esm/index.mjs",
  "types": "./dist/common/index.d.ts",
  "exports": {
    ".": {
      "source": "./src/index.ts",
      "require": {
        "types": "./dist/common/index.d.ts",
        "default": "./dist/common/index.js"
      },
      "import": {
        "types": "./dist/esm/index.d.mts",
        "default": "./dist/esm/index.mjs"
      }
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "src",
    "CHANGELOG.md"
  ],
  "scripts": {
    "clean": "rm -rf tsconfig.tsbuildinfo ./dist",
    "dev": "yarn build --watch",
    "build": "yarn build:clear && tsup",
    "build:clear": "rm -rf ./dist",
    "build:tsup": "tsup ./src/index.ts --format esm,cjs --sourcemap",
    "build:types": "tsc --build",
    "prepublishOnly": "yarn build",
    "prettier:check": "prettier -c --ignore-unknown .",
    "prettier:fix": "prettier -w --ignore-unknown ."
  },
  "peerDependencies": {
    "@aptos-labs/ts-sdk": "^1.33.1",
    "@aptos-labs/wallet-adapter-core": "^4.23.0",
    "@aptos-labs/wallet-standard": "^0.2.0",
    "@wallet-standard/core": "^1.1.0"
  },
  "devDependencies": {
    "@aptos-labs/ts-sdk": "^1.33.1",
    "@aptos-labs/wallet-adapter-core": "^4.23.0",
    "@aptos-labs/wallet-standard": "^0.2.0",
    "@wallet-standard/core": "^1.1.0",
    "prettier": "^2.5.1",
    "tsup": "^7.1.0",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "@mizuwallet-sdk/core": "^1.4.0",
    "@telegram-apps/bridge": "^1.9.1",
    "graphql": "^16.10.0",
    "graphql-request": "^7.1.2"
  }
}
